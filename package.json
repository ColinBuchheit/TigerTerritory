{
  "name": "tigerterritory",
  "version": "1.0.0",
  "description": "Tiger Territory Sports Website",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "build": "cd client && npm install && npm run build",
    "client": "cd client && npm start",
    "server": "cd server && node app.js",
    "dev:client": "cd client && npm start",
    "dev:server": "cd server && node app.js",
    "prod:local": "npm run build && set NODE_ENV=production && node server.js"
  },
  "engines": {
    "node": "18.x"
  },
  "dependencies": {
    "express": "^4.18.2",
    "compression": "^1.7.4",
    "helmet": "^8.1.0",
    "http-proxy-middleware": "^2.0.6",
    "mongoose": "^8.13.2",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.2.1",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "morgan": "^1.10.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.0"
  }
}